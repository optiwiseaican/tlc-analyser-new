<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:text=""
        android:background="@color/black"
        android:textColor="@color/black"
        android:textSize="@dimen/cntr_text_size" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:text=""
            android:background="@color/black"
            android:textColor="@color/black"
            android:textSize="@dimen/cntr_text_size" />

        <TextView
            android:id="@+id/contourId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ID"
            android:textColor="@color/black"
            android:padding="5dp"
            android:layout_weight="1"
            android:textSize="@dimen/cntr_text_size" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:text=""
            android:background="@color/black"
            android:textColor="@color/black"
            android:textSize="@dimen/cntr_text_size" />

        <TextView
            android:id="@+id/rfValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rf"
            android:textColor="@color/black"
            android:padding="5dp"
            android:layout_weight="1"
            android:textSize="@dimen/cntr_text_size" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:text=""
            android:background="@color/black"
            android:textColor="@color/black"
            android:textSize="@dimen/cntr_text_size" />
        <TextView
            android:id="@+id/areaValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Area"
            android:textColor="@color/black"
            android:padding="5dp"
            android:layout_weight="1"
            android:textSize="@dimen/cntr_text_size" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:text=""
            android:background="@color/black"
            android:textColor="@color/black"
            android:textSize="@dimen/cntr_text_size" />
        <TextView
            android:id="@+id/volumeValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Volume"
            android:layout_weight="1"
            android:textColor="@color/black"
            android:padding="5dp"
            android:textSize="@dimen/cntr_text_size" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:text=""
            android:background="@color/black"
            android:textColor="@color/black"
            android:textSize="@dimen/cntr_text_size" />


    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:text=""
        android:background="@color/black"
        android:textColor="@color/black"
        android:textSize="@dimen/cntr_text_size" />


</LinearLayout>